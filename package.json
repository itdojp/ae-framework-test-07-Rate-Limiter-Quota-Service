{
  "name": "ae-framework-test-07-rate-limiter-quota-service",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Rate limiter / quota service for ae-framework evaluation",
  "engines": {
    "node": ">=20.11 <23"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:artifacts": "vitest run --reporter=json --outputFile artifacts/summary/vitest-summary.json",
    "test:acceptance": "vitest run tests/acceptance.spec.ts --reporter=json --outputFile artifacts/summary/acceptance-summary.json",
    "test:property": "vitest run tests/property.spec.ts --reporter=json --outputFile artifacts/summary/property-summary.json",
    "test:mbt": "vitest run tests/mbt.spec.ts --reporter=json --outputFile artifacts/summary/mbt-summary.json",
    "test:persistence": "vitest run tests/persistence.spec.ts --reporter=json --outputFile artifacts/summary/persistence-summary.json",
    "test:e2e:restart": "vitest run tests/e2e-restart.spec.ts --reporter=json --outputFile artifacts/summary/e2e-restart-summary.json",
    "test:load": "tsx scripts/automation/run-load-check.ts",
    "test:mutation:report": "bash scripts/automation/run-mutation-check.sh",
    "mutation": "node scripts/automation/run-mutation-synthetic.mjs",
    "formal:check": "bash scripts/automation/run-formal-check.sh",
    "report:acceptance": "node scripts/automation/generate-acceptance-report.mjs",
    "pipeline:local": "bash scripts/automation/run-local-pipeline.sh"
  },
  "dependencies": {
    "fastify": "^5.6.1"
  },
  "devDependencies": {
    "@types/node": "^22.18.0",
    "fast-check": "^3.23.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^2.1.9"
  }
}
